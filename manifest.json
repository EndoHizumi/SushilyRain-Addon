{
    "manifest_version": 2,
    "name": "SushilyRain",
    "version": "1.0",
  
    "description": "好きなサイトで寿司が降ります",
  
    "icons": {
      "48": "icons/icon-48.jpg",
      "96": "icons/icon-96.jpg"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["src/main.js"]
      }
    ],
    "permissions": [
      "activeTab"
    ]
  }
  